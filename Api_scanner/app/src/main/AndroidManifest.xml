<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <!-- Permiso a internet para conectarse a la bbdd -->
    <uses-permission android:name="android.permission.INTERNET" /> <!-- Permiso a camera para poder escanear QR -->
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label=""
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Api_scanner"
        tools:targetApi="31">
        <activity
            android:name=".MainLogin"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity

            android:name=".altaPrinter.MainGestionAlmacen"
            android:exported="false" />
        <activity
            android:name=".printer.MainChangeUbicationPrinter"
            android:exported="true" />

        <activity
            android:name=".printer.MainUbicationPrinter"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true">

        </activity>
        <activity
            android:name=".scanner.MainScanner"
            android:exported="true" />
    </application>

</manifest>